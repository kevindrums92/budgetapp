{
  "name": "budget-app",
  "private": true,
  "version": "0.16.0",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite",
    "dev:prod": "vite --mode production",
    "build": "tsc -b && vite build",
    "build:dev": "tsc -b && vite build --mode development",
    "build:prod": "tsc -b && vite build --mode production",
    "cap:sync": "npx cap sync",
    "configure:dev": "node scripts/configure-env.cjs development",
    "configure:prod": "node scripts/configure-env.cjs production",
    "ios:dev": "npm run configure:dev && npm run build:dev && npx cap sync ios && npx cap open ios",
    "ios:prod": "npm run configure:prod && npm run build:prod && npx cap sync ios && npx cap open ios",
    "android:dev": "npm run configure:dev && npm run build:dev && npx cap sync android && npx cap open android",
    "android:prod": "npm run configure:prod && npm run build:prod && npx cap sync android && npx cap open android",
    "lint": "eslint .",
    "verify": "npm run lint && npm run test:run && npm run build",
    "preview": "vite preview",
    "start": "node server.js",
    "heroku-postbuild": "npm run build",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:critical": "playwright test e2e/01-onboarding-flow e2e/02-transaction-management e2e/06-cloud-sync",
    "test:e2e:report": "playwright show-report",
    "release": "bash scripts/release.sh",
    "dev:sync": "git pull origin develop",
    "dev:save": "git add . && git commit && git push origin develop",
    "dev:update-from-main": "git checkout develop && git merge main && git push origin develop",
    "diff:main": "git diff main..develop",
    "pre-release": "git checkout develop && git pull && npm run build && npm run lint && npm run test:run && npm run test:e2e",
    "git:ignore-env": "git update-index --skip-worktree capacitor.config.ts ios/App/App/Info.plist ios/App/App.xcodeproj/project.pbxproj ios/App/App/App.entitlements",
    "git:track-env": "git update-index --no-skip-worktree capacitor.config.ts ios/App/App/Info.plist ios/App/App.xcodeproj/project.pbxproj ios/App/App/App.entitlements"
  },
  "dependencies": {
    "@capacitor-community/admob": "^8.0.0",
    "@capacitor-firebase/messaging": "^8.0.1",
    "@capacitor/android": "^8.0.1",
    "@capacitor/app": "^8.0.0",
    "@capacitor/browser": "^8.0.0",
    "@capacitor/camera": "^8.0.0",
    "@capacitor/cli": "^8.0.1",
    "@capacitor/core": "^8.0.1",
    "@capacitor/filesystem": "^8.1.0",
    "@capacitor/ios": "^8.0.1",
    "@capacitor/network": "^8.0.0",
    "@capacitor/share": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@capgo/capacitor-native-biometric": "^8.3.2",
    "@fingerprintjs/fingerprintjs": "^5.0.1",
    "@ionic/pwa-elements": "^3.3.0",
    "@revenuecat/purchases-capacitor": "^12.1.0",
    "@supabase/supabase-js": "^2.89.0",
    "browser-image-compression": "^2.0.2",
    "capacitor-voice-recorder": "^7.0.6",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "express": "^5.2.1",
    "firebase": "^12.8.0",
    "i18next": "^25.8.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.3",
    "react-router-dom": "^7.12.0",
    "recharts": "^3.6.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^25.0.1",
    "jsonwebtoken": "^9.0.3",
    "msw": "^2.12.7",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^2.1.9"
  }
}
